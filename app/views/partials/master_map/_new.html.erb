<h1>Create a new Strategy Map</h1>

<% form_for(@master_map) do |f| %>
<%= f.error_messages %>
<p>
	<%= f.label :name %><br />
	<%= f.text_field :name %>
</p>

  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description, :rows=> 4 %>
  </p>
  
  <p>
    <%= f.label :created_by , "Creator"%><br />
    <%= f.text_field :created_by, :value=>findCurrentUser(session[:user_id]) %>
  </p>
<p>
	<%= f.submit 'Create' %>
</p>
<% end %>
